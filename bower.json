{
	"name": "gasoline",
	"version": "0.0.0",
	"private": "true",
	"dependencies": {
		"jquery": "*",
		"bootstrap": "*",
		"bootstrap-datepicker": "*",
		"font-awesome": "*",
		"jquery-timeago": "*"
	},
	"exportsOverride": {
		"bootstrap": {
			"js": "dist/js/*.js",
			"css": [ "dist/css/bootstrap.css",  "dist/css/bootstrap.min.css" ],
			"fonts": "dist/fonts/*"
		},
		"bootstrap-datepicker": {
			"js": "js/*.js",
			"js/locales": "js/locales/*.js",
			"css": "css/datepicker3.css"
		},
		"font-awesome": {
			"css": [ "css/*.css", "css/*.min.css" ],
			"fonts": "fonts/*"
		},
		"jquery": {
			"js": [ "dist/*.js", "dist/*.min.js" ]
		},
		"jquery-timeago": {
			"js": "jquery.timeago.js",
			"js/locales": "locales/*.js"
		}
	}
}
